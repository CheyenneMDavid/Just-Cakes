{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "proseWrap": "preserve",
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "vueIndentScriptAndStyle": false,
  "plugins": ["@prettier/plugin-pug"]
}
